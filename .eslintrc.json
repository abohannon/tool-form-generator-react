{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true,
        "es6": true,
        "mocha": true
      },
    "rules": {
        "react/require-default-props": 0,
        "react/forbid-prop-types": 0,
        "react/button-has-type": 0,
        "func-names": 0,
        "quotes": [2, "backtick"],
        "no-plusplus": 0,
        "max-len": 0,
        "prefer-destructuring": 0,
        "no-underscore-dangle": 0,
        "comma-dangle": 0,
        "no-console": "off",
        "jsx-a11y/label-has-for": "off",
        "class-methods-use-this": 0,
        "semi": 0
    }
}